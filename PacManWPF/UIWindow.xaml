<Window x:Class="PacManWPF.UIWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PacManWPF"
        mc:Ignorable="d " PreviewKeyDown="DispatchKey" Closed="OnClosed"
        Title="Pacman" Width="1480" Height="720" Background="Black">
    <Window.Icon>
        <BitmapImage UriSource="Assets/Images/pacman.png"></BitmapImage>
    </Window.Icon>
    <Frame x:Name="CurrentPage" local:FrameNoHistoryBehavior.Enable="True" ></Frame>
</Window>
