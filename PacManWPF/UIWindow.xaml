                        </Grid.RowDefinitions>
                        <Image Stretch="Uniform" Grid.Row="0" HorizontalAlignment="Center" Source="Assets\Images\folder.png" VerticalAlignment="Top" Cursor="Hand"></Image>
                        <Label Grid.Row="1" Content="Apri  cartella  mondi" Foreground="Yellow" HorizontalAlignment="Center"></Label>
                    </Grid>
                    <Grid Grid.Column="1" Grid.Row="1" MouseLeftButtonDown="OpenSettings">
                        <Grid.RowDefinitions>
                            <RowDefinition></RowDefinition>
                            <RowDefinition Height="20"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Image Stretch="Uniform" Grid.Row="0" HorizontalAlignment="Center" Source="Assets\Images\settings.png" VerticalAlignment="Top" Cursor="Hand"></Image>
                        <Label Grid.Row="1" Content="Impostazioni/Start" Foreground="Yellow" HorizontalAlignment="Center"></Label>
                    </Grid>
                    <Grid Grid.Column="2" Grid.Row="1" MouseLeftButtonDown="OpenTG">
                        <Grid.RowDefinitions>
                            <RowDefinition></RowDefinition>
                            <RowDefinition Height="20"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Image Stretch="Uniform" HorizontalAlignment="Center" Source="Assets\Images\tg.png" VerticalAlignment="Top" Cursor="Hand"></Image>
                        <Label Grid.Row="1" Content="Sviluppatore" Foreground="Yellow" HorizontalAlignment="Center"></Label>
                    </Grid>
                    <Grid Grid.Column="3" Grid.Row="1" MouseLeftButtonDown="OpenGit">
                        <Grid.RowDefinitions>
                            <RowDefinition></RowDefinition>
                            <RowDefinition Height="20"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Image Stretch="Uniform" HorizontalAlignment="Center" Source="Assets\Images\Git.png" Cursor="Hand"></Image>
                        <Label Grid.Row="1" Content="Codice  sorgente" Foreground="Yellow" HorizontalAlignment="Center"></Label>
                    </Grid>
                    <Grid Grid.Column="4" Grid.Row="1" MouseLeftButtonDown="OpenKaze">
                        <Grid.RowDefinitions>
                            <RowDefinition></RowDefinition>
                            <RowDefinition Height="20"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Image Grid.Row="0" Stretch="Uniform" HorizontalAlignment="Center" Source="Assets\Images\Kaze94.png" Cursor="Hand" DockPanel.Dock="Top"></Image>
                        <Label Grid.Row="1" Content="Grafico  Kaze94" Foreground="Yellow" HorizontalAlignment="Center"></Label>
                    </Grid>
                </Grid>
            </DockPanel>
        </TabItem>
