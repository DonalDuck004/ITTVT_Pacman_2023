        <TabItem Header="PauseTab" Name="pause_menu_tab" TabIndex="1" Visibility="Collapsed">
            <Border Background="GhostWhite" BorderBrush="Gainsboro" BorderThickness="1" Width="490" Height="245" HorizontalAlignment="Center" >
                <StackPanel Background="Black" Width="480" Height="240">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                        </Grid.RowDefinitions>
                        <Label Content="PAUSA" Foreground="Yellow" FontSize="20" HorizontalAlignment="Stretch" HorizontalContentAlignment="Center" Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2"/>
                        <StackPanel Grid.Row="1" Grid.Column="0">
                            <Label Margin="0,10,0,0"  Content="Mondi" Foreground="Yellow" HorizontalAlignment="Left" HorizontalContentAlignment="Center"/>
                            <ListBox Foreground="White" Background="#FF000137" HorizontalAlignment="Stretch" Height="123" Name="worlds_box" SelectionChanged="OnWorldSelected"></ListBox>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Button Content="Aggiorna" Click="DropWorldsCache" Grid.Column="0"></Button>
                                <Button Content="Ottieni nuovi mondi" Grid.Column="1"  Click="OpenExt"></Button>
                            </Grid>
                        </StackPanel>
                        <StackPanel Grid.Row="1" Grid.Column="1" >
                            <Label Margin="0,10,0,0" Content="Impostazioni" Foreground="Yellow" HorizontalAlignment="Left"/>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="70"></ColumnDefinition>
                                    <ColumnDefinition></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                </Grid.RowDefinitions>

                                <Label Margin="1,0,0,0" Grid.Row="0" Grid.Column="0" Content="Volume" Foreground="Yellow" HorizontalAlignment="Left" HorizontalContentAlignment="Right" VerticalAlignment="Center"/>
                                <Slider Grid.Row="0" Grid.Column="1" Width="120" VerticalAlignment="Center" HorizontalAlignment="Left" ValueChanged="SetVolume" Maximum="100" Minimum="0" Value="{x:Static game:RuntimeSettingsHandler.XAML_Volume}"/>
                                <Label Margin="1,0,0,0" Grid.Row="1" Grid.Column="0" Content="Grafica" Foreground="Yellow" HorizontalAlignment="Left" HorizontalContentAlignment="Right" VerticalAlignment="Center"/>
                                <Slider Grid.Row="1" Grid.Column="1" Width="120" VerticalAlignment="Center" HorizontalAlignment="Left" ValueChanged="SetGraphicsQuality"  Maximum="3" Minimum="0" IsSnapToTickEnabled="True" Ticks="0,1,2,3" Value="{x:Static game:RuntimeSettingsHandler.XAML_GraphicMode}"/>
                                <Label Grid.Row="2" Grid.Column="1" Content="UNSPECIFIED" Foreground="Yellow" VerticalAlignment="Center" HorizontalAlignment="Left" Name="quality_label"></Label>
                                <CheckBox Grid.Row="3" Grid.ColumnSpan="2" Content="Animations" Foreground="Yellow" VerticalAlignment="Center" IsChecked="{x:Static game:RuntimeSettingsHandler.XAML_AnimationsEnabled}" Checked="SetAnimations" Unchecked="UnSetAnimations"></CheckBox>
                            </Grid>
                        </StackPanel>
                        <Button Margin="0,10,0,0" Width="100" Click="ClosePauseMenu" Content="Chiudi" Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="2"></Button>
                    </Grid>
                </StackPanel>
            </Border>
        </TabItem>
