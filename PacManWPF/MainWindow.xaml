            <DockPanel>
                <DockPanel.Background>
                    <ImageBrush ImageSource="Assets/Images/bg.png" Stretch="Uniform">
                        
                    </ImageBrush>
                </DockPanel.Background>
                <Grid Margin="0, 0, 0, 6">
                    <Grid.RowDefinitions>
                        <RowDefinition></RowDefinition>
                        <RowDefinition Height="90"></RowDefinition>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition></ColumnDefinition>
                        <ColumnDefinition></ColumnDefinition>
                        <ColumnDefinition></ColumnDefinition>
                        <ColumnDefinition></ColumnDefinition>
