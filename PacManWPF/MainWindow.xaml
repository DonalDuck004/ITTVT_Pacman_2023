        <TabItem Header="GameWonTab" TabIndex="2" Name="game_won_tab" Visibility="Collapsed">
            <DockPanel>
                <WrapPanel Name="game_won" VerticalAlignment="Center" HorizontalAlignment="Center" DockPanel.Dock="Top">
                    <Label Foreground="Yellow" FontSize="100" Content="Mondo "></Label>
                    <Label Name="game_won_label" Foreground="Yellow" FontSize="100" Content="Sample"></Label>
                    <Label Foreground="Yellow" FontSize="100" Content="Superato!"></Label>
                </WrapPanel>
                <Grid Width="auto" DockPanel.Dock="Bottom" VerticalAlignment="Bottom">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition></ColumnDefinition>
                        <ColumnDefinition></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Label HorizontalAlignment="Left" Grid.Column="0" Content="Premi  esc  per  tornare  alla  scelta  dei  mondi" Foreground="Yellow" FontSize="17" />
                    <WrapPanel HorizontalAlignment="Right" Grid.Column="1">
                        <Label Content="Tempo  impiegato:" Foreground="Yellow" FontSize="17" />
                        <Label x:Name="ellapsed_time_label" Content="00:00:00" Foreground="Yellow" FontSize="17" />
                    </WrapPanel>
                </Grid>
            </DockPanel>
        </TabItem>
