        <TabItem Header="PauseTab" Name="pause_menu_tab" TabIndex="1" Visibility="Collapsed">
            <Border Background="GhostWhite" BorderBrush="Gainsboro" BorderThickness="1" Width="490" Height="245" HorizontalAlignment="Center" >
                <StackPanel Background="Black" Width="480" Height="240">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                            <RowDefinition></RowDefinition>
                        </Grid.RowDefinitions>
                        <Label Content="PAUSA" Foreground="Yellow" FontSize="20" HorizontalAlignment="Stretch" HorizontalContentAlignment="Center" Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2"/>
                        <StackPanel Grid.Row="1" Grid.Column="0">
                            <Label Margin="0,10,0,0"  Content="Mondi" Foreground="Yellow" HorizontalAlignment="Left" HorizontalContentAlignment="Center"/>
                            <ListBox Foreground="White" Background="#FF000137" HorizontalAlignment="Stretch" Height="123" Name="worlds_box" SelectionChanged="OnWorldSelected"></ListBox>
                            <Button Content="Aggiorna" Click="DropWorldsCache"></Button>
                        </StackPanel>
                        <StackPanel Grid.Row="1" Grid.Column="1" >
                            <Label Content="Impostazioni" Foreground="Yellow" HorizontalAlignment="Left"/>
                            <WrapPanel Margin="10,0,0,0">
                                <Label Content="Volume" Foreground="Yellow" HorizontalAlignment="Left" HorizontalContentAlignment="Center"  VerticalAlignment="Center"/>
                                <Slider Width="120" VerticalAlignment="Center" ValueChanged="SetVolume" Value="100" Maximum="100" Minimum="0"/>
                            </WrapPanel>
                            <!--
                            <Label Margin="10,0,0,0" Content="Grafica" Foreground="Yellow" HorizontalAlignment="Left"/>
                            <WrapPanel Margin="20,0,0,0">
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                        <RowDefinition></RowDefinition>
                                    </Grid.RowDefinitions>
                                    <RadioButton Content="LowQuality" Foreground="Yellow" Grid.Row="0"></RadioButton>
                                    <RadioButton Content="Fant" Foreground="Yellow" Grid.Row="1"></RadioButton>
                                    <RadioButton Content="Linear" Foreground="Yellow" Grid.Row="2"></RadioButton>
                                    <RadioButton Content="HighQuality" Foreground="Yellow" Grid.Row="3"></RadioButton>
                                </Grid>
                            </WrapPanel>-->
                        </StackPanel>
                        <Button Margin="0,10,0,0" Width="100" Click="ClosePauseMenu" Content="Chiudi" Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="2"></Button>
